{
  "name": "mmojournal-web",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc -p tsconfig.app.json --noEmit",
    "lint": "eslint \"app/**/*.{ts,tsx}\"",
    "lint:fix": "eslint . --fix",
    "format": "prettier -w .",
    "backend:dev": "cd backend && npm run dev",
    "backend:build": "cd backend && npm run build",
    "backend:start": "cd backend && npm start",
    "backend:db:generate": "cd backend && npx prisma generate",
    "backend:db:migrate": "cd backend && npx prisma migrate dev",
    "backend:db:studio": "cd backend && npx prisma studio",
    "sb:ping": "tsx scripts/sb-ping.ts",
    "audit:hunts": "tsx scripts/audit-hunts.ts",
    "audit:cases": "tsx scripts/check-cases.ts",
    "db:docker:start": "bash ./scripts/db_start_docker.sh",
    "db:docker:stop": "bash ./scripts/db_stop_docker.sh",
    "db:rebuild": "bash ./scripts/db_rebuild_local.sh"
  },
  "dependencies": {
    "@react-router/node": "^7.5.3",
    "@react-router/serve": "^7.5.3",
    "@supabase/supabase-js": "^2.50.3",
    "isbot": "^5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.3",
    "react-router-dom": "^7.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@react-router/dev": "^7.5.3",
    "@tailwindcss/vite": "^4.1.4",
    "@types/express": "^5.0.3",
    "@types/node": "^20.19.4",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "dotenv": "^17.2.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.3.3",
    "tailwindcss": "^4.1.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
